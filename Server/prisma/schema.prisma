generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user{
  id        Int @id @default(dbgenerated())
  username  String @unique
  name      String
  address   String
  bio       String?
}

model Post{
  id        Int @id
  title     String
}